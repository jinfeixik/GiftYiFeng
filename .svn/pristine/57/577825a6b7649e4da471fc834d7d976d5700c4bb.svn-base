		<script>
			$(function() {
				$('.pay-platform').on('click', '.fl', function() {
					$(".pay-platform .fl").removeClass('focus');
					$(this).addClass('focus');
					var order_id = $('#order_id').val();
					var pay_id = $(this).data('pay_id');
					var self = this;
					$.ajax({
						type: "POST",
						async: false,
						dataType: 'json',
						url: "user.php",
						data: {
							act: 'change_payment',
							order_id: order_id,
							pay_id: pay_id
						},
						success: function(data) {
							if(data.success) {
								var paycode = $(self).data('paycode');
								if(paycode == 'wx_scan') {
									var img = $('#img_code').parent().html();
									var html = '<div class="wx-pay rel"><img src="/themes/pc/images/sao.png" class="sao"><p class="price f20">支付 ￥69.00</p>\
						<div class="tc ewm-box">\
						<p class="f24">微信扫码支付</p>\
						<p><span class="ewm dib b-1">' + img + '</span></p>\
						<p class="f14 lh2"><span class="f-d93732"><span id="timer">59</span>秒</span>后此二维码过期</p>\
						</div></div>';
									art.dialog({
										padding: 0,
										title: '',
										content: html,
										lock: true,
										background: "#000000",
										opacity: 0.15
									});
									$("#timer").countDown(300, function(flag) {
										if(flag) {
											$(arguments[2]).html(arguments[1]);
										} else {
											$(arguments[1]).parent().parent().html("二维码已失效");
										}
									});
								} else {
									var $pay_button = $(self).find('.pay_btn');
									//console.log($pay_button);
									if($pay_button.prop('tagName') == 'A') {
										var href = $pay_button.attr('href');
										window.open(href);
									} else {

										if($pay_button.attr('type') == 'submit') {
											$pay_button.parents('form').submit();
										} else if($pay_button.attr('type') == 'button') {
											eval($pay_button.attr('onclick'));
										}
									}
								}
							} else {
								alert(data.error);
							}

						}
					});
				});

			});
		</script>
</body>

</html>