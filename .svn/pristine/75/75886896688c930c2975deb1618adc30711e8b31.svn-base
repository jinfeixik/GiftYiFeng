@CHARSET "UTF-8";
body{
	background-image:image/login_background.jpg;
	background-color: #feeeed;
	width:100%;
	height: 100%;
}

.register{
	width:600px;
	background-color: #fff;
	float:right;
	margin:50px 100px 0px 0px;

}

.register_context_center{
	padding:50px 100px;
}
.register_div1{
	width:100%;
	height:50px;
	
	
}

.register_title{
	font-size: 24px;
	float: left;

}
.notice{
	color:red;
	float:right;  

}
input{
	padding:10px;
	width:100%;
	
}
.username{

	border: 1px solid #eee;
}
.password{
 
	border:1px solid #eee;
}

.register_submit{
	background:#d93732;
	border: #d93732;
	color: #fff;
	padding:11px;
}
.no_account{
float:left;
	
}
a{text-decoration:none;color:#000;}
a:hover{text-decoration:none}

.register_foot{
	margin-top:15px;
	margin-bottom:30px;
}

.a_register{
	color:#cb9750;
}
.register{
	background:#fff;
}
.safety_verification{
	background:#ccc;
	padding:10px;
	text-decoration:none;
	color:#666;
}

#test {
    width:100%;
    height:100%;
    background-color:#000;
    position:absolute;
    top:0;
    left:0;
    z-index:2;
    opacity:0;
    /*兼容IE8及以下版本浏览器*/
    filter: alpha(opacity=30);
    display:none;
}

#log_window {
    width:0px;
    height:0px;
    background-color:#fff;    
    margin: auto;
    border:1px solid #ccc;
    position: absolute;
    z-index:3;
    top: 0;
    bottom: 0;
    left: -200px;
    right: 0;
    display:none;
 
}
