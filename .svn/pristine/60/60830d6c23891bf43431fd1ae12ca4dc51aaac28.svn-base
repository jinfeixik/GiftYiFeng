@CHARSET "UTF-8";
body{
	padding:40px 150px;
	background: #f5f5f5;	
}
#form{
	height:90%;
	background:#fff;
	padding:60px;
}
.left_td{
	width:25%;
	height:90%;
	border-right:1px solid #eee;
	float: left;
}
.left_td img{
	width:66px;
    padding-left:100px;
}
.right_td{
	width:65%;
	height:90%;
	float:left;
	padding:0px 0px 0px 80px;
}
input{
	padding:15px;
	width:400px;
	margin-top:15px;
	border:1px solid #eee;
}
#code{
	width:250px;
	float:left;
}
.getCode{
	width:150px;
	float: left;
	background: #d93732;
	color:#fff;
}
.submit{
	background: #d93732;
	color:#fff;
}